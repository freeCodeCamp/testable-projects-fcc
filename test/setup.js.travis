/**
 * Mocha setup for Travis CI.
 *
 * This file will be copied to setup.js by Travis.
 *
 */

// Location of chrome binary.
global.chromeBinaryPath = '/usr/bin/google-chrome-stable';

// Where to save screenshots.
global.screenshotDir = 'travis-screenshots';

// URL for local bundle.js
global.bundleUrl = 'https://127.0.0.1:8080/bundle.js';

// Browser will be resized using these values.
global.browserMaxWidth = 1920;
global.browserMaxHeight = 1080;
