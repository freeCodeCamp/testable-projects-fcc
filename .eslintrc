{
  "root": true,
  "extends": ["@freecodecamp/eslint-config", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "max-len": [
      "error",
      { "code": 100, "ignoreUrls": true, "ignoreTemplateLiterals": true }
    ],
    "prettier/prettier": "error"
  },
  "settings": {
    "react": {
      "version": "16.4.2"
    }
  }
}
